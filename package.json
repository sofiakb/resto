{
  "name": "@sofiakb/resto",
  "version": "1.0.0-dev.11",
  "description": "A typescript library for axios API calls.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "rm -rf lib && eslint 'src/**/*.{ts,tsx}' --quiet --fix",
    "prepare": "pnpm run lint && pnpm run format && pnpm run build",
    "prepublishOnly": "pnpm run lint",
    "release": "./scripts/release.sh",
    "preversion": "pnpm run lint",
    "version": "pnpm run test && pnpm run build && pnpm run format && git add -A package.json src && pnpm publish --access public",
    "postversion": "git push -u origin HEAD --tags && ./scripts/release.sh $(cat package.json | jq -r '.version')"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sofiakb/resto.git"
  },
  "author": {
    "name": "Sofiakb",
    "email": "<contact.sofiakb@gmail.com>",
    "url": "https://sofianeakbly.fr"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sofiakb/resto/issues"
  },
  "homepage": "https://github.com/sofiakb/resto#readme",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "babel-jest": "^28.0.8",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.0.8",
    "lodash": "^4.17.21",
    "prettier": "^2.8.4",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "axios": "^0.27.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.37",
    "pluralize": "^8.0.0"
  }
}
